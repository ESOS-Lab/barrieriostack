	pinctrl@14CE0000 {
		attn_irq: attn-irq {
			  samsung,pins = "gpj1-2";
			  samsung,pin-function = <0xf>;
			  samsung,pin-pud = <0>;
			  samsung,pin-drv = <0>;
		};

		attn_input: attn-input {
			  samsung,pins = "gpj1-2";
			  samsung,pin-function = <0>;
			  samsung,pin-pud = <1>;
			  samsung,pin-drv = <0>;
		};
	};

	i2c@14E10000 {
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		#size-cells = <0>;
		status = "okay";
		touchscreen@20 {
			compatible = "synaptics,rmi4";
			reg = <0x20>;
			pinctrl-names = "on_state", "off_state";
			pinctrl-0 = <&attn_irq>;
			pinctrl-1 = <&attn_input>;
			synaptics,irq_gpio = <&gpj1 2 0>;
			synaptics,irq_type = <8200>;
			synaptics,max_coords = <1079 1919>;	/* x y */
			synaptics,num_lines = <29 16>;		/* rx tx */

			synaptics,regulator_dvdd = "tsp_io";
			synaptics,regulator_avdd = "tsp_avdd";
			synaptics,project_name = "";
		};


	};

	i2c@6b {
		compatible = "i2c-gpio";
		gpios = <&gpf0 2 0 /* sda */
			 &gpf0 3 0 /* scl */
			>;
		i2c-gpio,delay-us = <2>;	/* ~100kHz */
		#address-cells = <1>;
		#size-cells = <0>;
		status = "okay";

		bq24160_charger@6b {
			compatible = "ti,bq24160";
			reg = <0x6b>;
			gpios = <&gpa0 0 0xf>;
			pinctrl-names = "default";
			pinctrl-0 = <&chg_irq>;
			dev_name = "bq24160";
			support_boot_charging = <0>;
		};
	};

	hsi2c_1: hsi2c@14E50000 {
		compatible = "samsung,exynos5-hsi2c";
		clock-frequency = <400000>;	/* 400KHz */
		status = "okay";
		audio_codec: codec@3e {
			compatible = "audience,es515";
			reg = <0x3E>;
			gpios = <&gpg3 4 0	/* reset */
				 &gpg3 5 0	/* wakeup */
				 &gpa0 5 0xF>;	/* interrupt */
			pinctrl-names = "default";
			pinctrl-0 = <&codec_ctl &codec_irq>;
		};
	};

	i2c@14EA0000 {
		gpios = <&gpg3 0 0 &gpg3 1 0 >;
		status = "okay";
		samsung,i2c-sda-delay = <100>;
		samsung,i2c-max-bus-freq = <400000>;
		status = "okay";
		fimc-is-vision@20 {
			compatible = "samsung,exynos5-fimc-is-sensor-6b2";
			reg = <0x20>;
		};
	};

	pinctrl@14CC0000 {
		codec_ctl: codec-ctl {
			samsung,pins = "gpg3-4", "gpg3-5";
			samsung,pin-con-pdn = <3>;
		};
	};

	pinctrl@10580000 {
		codec_irq: codec-irq {
			samsung,pins = "gpa0-5";
			samsung,pin-con-pdn = <3>;
		};
	};

	spi_0: spi@14d20000 {
		status = "disabled";
	};

	spi_1: spi@14d30000 {
		status = "disabled";
	};

	spi_2: spi@14d40000 {
		status = "disabled";
	};

	/*disp_driver {*/
	decon_fb: decon_fb {
		lcd_info = <&s6e3fa0>;

		decon: decon_ctrl {
			samsung,max_bpp = <32>;
			samsung,default_bpp = <24>;

			fb_driver_data {
				fb_win_variant_0 {
					has_osd_c = <1>;
					has_osd_d = <1>;
					osd_size_off = <0x08>;
					palette_size = <256>;
					VALID_BPP_1248 = <1>;
					VALID_BPP_13 = <1>;
					VALID_BPP_15 = <1>;
					VALID_BPP_16 = <1>;
					VALID_BPP_18 = <1>;
					VALID_BPP_19 = <1>;
					VALID_BPP_24 = <1>;
					VALID_BPP_25 = <1>;
					VALID_BPP_32 = <1>;
				};
				fb_win_variant_1 {
					has_osd_c = <1>;
					has_osd_d = <1>;
					has_osd_alpha = <1>;
